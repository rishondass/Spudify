<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <div>
      <form action="/api/music" method="post" enctype="multipart/form-data">
        <input type="file" name="music_file" />
        <!-- <input type="text" name="test" id="file"> -->
        <button type="submit">Upload File</button>
      </form>
    </div>
    <div>
      <!-- <div>
        <video controls width="500px">
          <source src="/api/music/movie" />
        </video>
      </div> -->
      <%for(let i = 0; i < music.length; i++){%>
        <div>
          <div><%=music[i].title%></div>
          <div><%=music[i].songID%></div>
          <img src="<%=`/api/music/artwork/${music[i].songID}`%>" width="120px" height="120px" alt="song_pic"/>
        </div>
        
        <audio controls>
          <source src="<%=`/api/music/${music[i].songID}`%>">
        </audio>
      <%}%>
    </div>
  </body>
</html>
